{
  //*=== TELEMETRY ===*/
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.enableExperiments": false,
  //*=== WORKBENCH ===*/
  "workbench.colorTheme": "Solarized Light",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": false,
  "window.openFilesInNewWindow": "off",
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 0,
  "breadcrumbs.enabled": true,
  //*=== EDITOR - FORMATTERS ===*/
  "editor.maxTokenizationLineLength": 30000,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.fontFamily": "'Fira Code Medium', 'Fira Code', hack, 'Droid Sans Mono', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 20,
  "editor.fontWeight": "200",
  "editor.renderWhitespace": "none",
  "editor.insertSpaces": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "gutter",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.lineHeight": 30,
  "editor.rulers": [80],
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "same",
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": false,
  "editor.quickSuggestionsDelay": 4,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.minimap.enabled": false,
  "editor.matchBrackets": true,
  //*=== FILES-SEARCH ===*/
  "files.autoSave": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/*.iml": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "venv": true,
    "**/node_modules/*/**": true,
    "node_modules": true,
    "**/bower_components/**": true,
    "**/.idea/**": true,
    "**/.vscode": true,
    "env*": true,
    "*.sublime-*": true
  },
  "search.exclude": {
    "**/.idea/**": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true,
    "**/.git": true
  },
  "files.associations": {
    "*.html": "html",
    "*.njk": "html",
    "*.mustache": "html",
    "*vue": "vue",
    "*rs": "rust",
    "*ctl": "shellscript",
    "*conf": "shellscript",
    "*conf.js": "webpack",
    "*.prettierrc": "json",
    "*.babelrc": "json",
    "*.postcssrc": "json",
    "*.eslintrc": "json",
    "*.postcss": "postcss",
    "*.js": "javascriptreact"
  },
  //*=== MARKDOWN-HTML-EMMET ===*/
  // Overwrite settings
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": true
  },
  "markdown.preview.fontSize": 17,
  "emmet.triggerExpansionOnTab": true,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": [
      "javascript",
      "javascriptreact",
      "babeljavascript",
      "typescript",
      "typescriptreact",
      "babel"
    ]
  },
  "html-css-class-completion.enableEmmetSupport": true,
  "html.format.endWithNewline": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //*=== INTELLICODE - PATH_INTELLISENSE ===*/
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "path-intellisense.mappings": {
    "path-intellisense.mappings": {
      "/": "${workspaceRoot}"
    }
  },
  //*=== PYTHON ===*/
  // "python.jediEnabled": false,
  "[python]": {},
  //*=== PHP ===*/
  // "php.validate.executablePath": "<php-path>",
  //*=== ELIXIR-ERLANG ===*/
  // "elixir.useElixirSense": false,
  //*=== JS - TYPESCRIPT ===*/
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  //*=== VUE ===*/
  "vetur.format.styleInitialIndent": true,
  //*=== ESLINT ===*/
  "eslint.packageManager": "yarn",
  "eslint.nodePath": "/usr/bin/node",
  "eslint.options": {
    "extensions": [".js", ".ts", ".tsx", ".json", ".vue", ".jsx"]
  },
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  //*=== PRETTIER ===*/
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.arrowParens": "always",
  "prettier.singleQuote": true,
  "prettier.printWidth": 85,
  "prettier.semi": true,
  //*=== LIVE_SERVER-REST_CLIENT ===*/
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.port": 6550,
  "liveServer.settings.root": "/",
  "liveServer.settings.CustomBrowser": "firefox:PrivateMode",
  "liveServer.settings.NoBrowser": false,
  "liveServer.settings.ignoreFiles": [
    "node_modules/**",
    "bower_components/**",
    ".idea/**",
    ".git/**",
    ".vscode/**",
    "**/*.iml",
    "**/*.scss",
    "**/*.sass"
  ],
  //*=== BASH-SHELLCHECK ===*/
  "shellcheck.disableVersionCheck": true,
  "shellcheck.executablePath": "/usr/bin/shellcheck",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  //*=== VIM ===*/
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    }
  ],
  //*=== GIT-TERMINAL ===*/
  "git.path": "/usr/bin/git",
  "terminal.integrated.shell.linux": "/usr/bin/bash",
  "terminal.integrated.shell.osx": "/usr/bin/bash",
  "terminal.integrated.shellArgs.osx": [],
  "terminal.integrated.scrollback": 800,
  "terminal.integrated.cwd": "",
  //*=== CSPELL ===*/
  "cSpell.language": "en",
  "cSpell.userWords": [
    "Helvetica",
    "QLHTTP",
    "URL's",
    "antialiased",
    "graphQLHTTP",
    "graphiql",
    "intellisense",
    "moebius",
    "monospace",
    "polyfill",
    "scrollback"
  ],
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "css",
    "go",
    "golang",
    "handlebars",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "org",
    "php",
    "plaintext",
    "python",
    "react",
    "restructuredtext",
    "rust",
    "text",
    "typescript",
    "typescriptreact",
    "vue",
    "yml"
  ],
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.importStatement": "warning"
}
